{
  "name": "nmp-frontend-libraries",
  "version": "0.0.0",
  "description": "Shared frontend libraries for NMP",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "copy": "npm run copy --workspaces --if-present",
    "publish": "npm run eik:publish:ci --workspaces --if-present"
  },
  "workspaces": [
    "packages/react-17",
    "packages/react-dom-17",
    "packages/react-18",
    "packages/react-dom-18",
    "packages/trackjs",
    "packages/vue-3",
    "packages/vue-2",
    "packages/lit",
    "packages/lit-2",
    "packages/lit-3",
    "packages/lit-element",
    "packages/lit-html",
    "packages/podium-browser",
    "packages/lit-labs-ssr-client",
    "packages/warp-ds-elements-core",
    "packages/warp-ds-elements",
    "packages/warp-ds-react",
    "packages/warp-ds-vue",
    "packages/warp-ds-css"
  ],
  "devDependencies": {
    "@eik/cli": "2.0.32",
    "findup-sync": "5.0.0",
    "semver": "7.6.0"
  }
}
